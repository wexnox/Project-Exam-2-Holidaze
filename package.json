    "react-datepicker": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-icons": "^5.0.1",
    "react-navigation": "^5.0.0",
    "react-router-dom": "^6.21.2",
    "yup": "^1.3.3"
